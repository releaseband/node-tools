{
  "name": "@releaseband/jarvis",
  "version": "0.1.0",
  "description": "The Releaseband Team's Jarvis helper.",
  "author": "The Releaseband Team",
  "homepage": "https://releaseband.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/releaseband/node-tools.git",
    "directory": "packages/jarvis"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "jarvis": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "execa": "^4.0.3",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.35",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "eslint": "^7.10.0",
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jarvis",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
